            "tex": "\\hrulefill",
            "json": {
                "type": "hr"
            }
        }
    },

    {
        "name": "Thematic Break with Text",
        "text": "a\n***\nb",
        "formats": {
            "md": "a\n\n______________________________________________________________________\n\nb",
            "html": "<p>a</p><hr /><p>b</p>",
            "tex": "a\n\n\\hrulefill\n\nb",
            "json": [
                {
                    "type": "paragraph",
                    "children": [
                        {
                            "type": "inline",
                            "children": [
                                {
                                    "type": "text",
                                    "text": "a"
                                }
                            ]
                        }
                    ]
                },
                {
                    "type": "hr"
                },
                {
                    "type": "paragraph",
                    "children": [
                        {
                            "type": "inline",
                            "children": [
                                {
                                    "type": "text",
                                    "text": "b"
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    },

    {
        "name": "Basic Blockquote",
        "text": "> Some\n> Quote",
        "formats": {
            "md": "> Some\n> Quote",
            "html": "<blockquote><p>Some\nQuote</p></blockquote>",
            "tex": "\\begin{quote}\nSome\nQuote\n\\end{quote}",
            "json": {
                "type": "blockquote",
                "children": [
                    {
                        "type": "paragraph",
                        "children": [
                            {
                                "type": "inline",
                                "children": [
                                    {
                                        "type": "text",
                                        "text": "Some"
                                    },
                                    {
                                        "type": "softbreak"
                                    },
                                    {
                                        "type": "text",
                                        "text": "Quote"
                                    }
                                ]
                            }
                        ]
                    }
                ]
            }
        }
    }

]
